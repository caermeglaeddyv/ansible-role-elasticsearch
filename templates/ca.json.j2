{
  "CN": "{{ elasticsearch_certs_ca_cn }}",
  "key": {
    "algo": "{{ elasticsearch_certs_ca_key_algo }}",
    "size": {{ elasticsearch_certs_ca_key_size }}
  },
  "names": [
  {% for name in elasticsearch_certs_ca_names -%}
  {
    "C": "{{ name.c }}",
    "L": "{{ name.l }}",
    "O": "{{ name.o }}",
    "OU": "{{ name.ou }}"
  }
  {% endfor -%}
 ]
}
